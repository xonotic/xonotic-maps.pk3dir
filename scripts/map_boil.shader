textures/map_boil/credit
{
	qer_editorimage textures/map_boil/credit
	surfaceparm trans
	surfaceparm nonsolid
	polygonOffset
	cull none
	nopicmip
	{
		map textures/map_boil/credit
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbgen identity
	}
}

